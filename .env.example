# WalletConnect (required for wallet auth)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Optional API base URL for the frontend. Leave empty to use same-origin.
VITE_API_BASE_URL=

# The Graph (optional indexed historical queries)
VITE_GRAPH_API_KEY=your_graph_gateway_key

# Alchemy (optional RPC/indexing)
VITE_ALCHEMY_API_KEY=your_alchemy_key
VITE_ALCHEMY_UNICHAIN_URL=https://unichain-mainnet.g.alchemy.com/v2/your_alchemy_key


# QuickNode (preferred Unichain RPC + streaming endpoint)
VITE_QUICKNODE_UNICHAIN_HTTP_URL=https://<subdomain>.unichain-mainnet.quiknode.pro/<token>
VITE_QUICKNODE_UNICHAIN_WSS_URL=wss://<subdomain>.unichain-mainnet.quiknode.pro/<token>

# Goldsky RPC (optional backup RPC)
VITE_GOLDSKY_RPC_URL=https://edge.goldsky.com/standard/evm/130?secret=your_goldsky_secret

# Internal project identifier (optional)
VITE_EBOX_PROJECT_ID=your_internal_project_id

# Backend-only secrets (set with wrangler secret for worker deploys)
# wrangler secret put COINGECKO_API_KEY
# wrangler secret put ALCHEMY_UNICHAIN_URL
# wrangler secret put GOLDSKY_RPC_URL

# The Graph API key (optional, enables Uniswap subgraph quote source before fallback)
# wrangler secret put GRAPH_API_KEY
# wrangler secret put UNISWAP_API_KEY
